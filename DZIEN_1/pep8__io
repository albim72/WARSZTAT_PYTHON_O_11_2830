marka = "Jeep"
model = "Cherokee"
rocznik = 2020

print(marka,model,rocznik,sep=" -- ")
print("samoch贸d: " + marka + " "+ model + " " + str(rocznik))

sam = "samoch贸d -> marka: {}, model: {}, rocznik: {}."
print(sam.format(marka,model,rocznik))

sam = "samoch贸d -> rocznik: {2}, marka: {0}, model: {1}."
print(sam.format(marka,model,rocznik))

